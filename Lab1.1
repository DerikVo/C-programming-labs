#include <stdio.h>
int main()
{

	int num1;
	int num2;
	printf("enter number1: ");
	scanf_s("%d", &num1);

	printf("enter number2: ");
	scanf_s("%d", &num2);

	printf("sum is %d\n", num1 + num2);

	if ((num1 + num2) % 2 == 0)
		printf("%d is even\n", num1 + num2);
	else
		printf("%d is odd\n", num1 + num2);
}
